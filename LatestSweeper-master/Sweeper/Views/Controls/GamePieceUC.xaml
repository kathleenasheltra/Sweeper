<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Sweeper.Views.Controls.GamePieceUC"
	x:Name="UserControl"
	d:DesignWidth="110" d:DesignHeight="90">

	<Grid x:Name="LayoutRoot" Width="110" Height="90">
        <Rectangle x:Name="PieceRectangle" Fill="{DynamicResource BUTTON}" HorizontalAlignment="Left" Height="90" Stroke="Black" VerticalAlignment="Top" Width="110"/>
    </Grid>
</UserControl>